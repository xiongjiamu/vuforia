$mod_nav()
  /* nav 开始 */
  .nav_all
    padding 0 15px
    height 50px
    background-color #3D4647
  .nav_box
    width 100%
    height 50px
    margin 0 auto
    overflow hidden
    .nav_logo
      width 60px
      height 50px
      line-height 50px
      img
        margin-top -4px
        width 100%
    .nav
      display none
      overflow hidden
      font-size 18px
      height 70px
      li
        margin 0
        a
          display inline-block
          height 70px
          background none
          border-radius 0
          line-height 70px
          padding 0 30px
          color #fff
          font-size 18px
      li:hover
        background #00AEEF
        a
          color #fff
      .activeN
        a
          color #5BB73B
    .nav_icon
      height 50px
      width auto
      display inline-block
      line-height 62px
      i
        font-size 34px
        color #fff
  .navAll_fixed
    position fixed
    width 100%
    top 0
    z-index 10000

  // 移动端
  .mobile_navBox
    width 0%
    height 100%
    padding-top 45px
    position fixed
    top 0
    right 0
    z-index 100000
    transition width 0.3s
    -moz-transition width 0.3s
    -webkit-transition width 0.3s
    -o-transition width 0.3s
  .mobile_nav
    width 0%
    height auto
    /*background rgba(0,0,0,0.85)*/
    position fixed
    right 0
    top 50px
    z-index 100002
    transition width 0.3s
    -moz-transition width  0.3s
    -webkit-transition width  0.3s
    -o-transition width  0.3s
    ul
      li
        background #00AEEF
        a
          display block
          width 100%
          height 42px
          border-radius 0
          line-height 42px
          text-align center
          font-size 18px
          color #fff
      .activeNM
        background #C5EEFF
        a
          color #0071C5
  .mobile_navBox.mobile_NBShow
    width  100%
  .mobile_nav.mobile_NShow
    width  80%


  @media (min-width :768px)
    .nav_all
      height 70px
      position fixed
      top 0
      width 100%
      z-index 3
      //background-color transparent
    .navAll_fixed
      top 0
      background-color rgba(0,0,0,.5)!important
    .nav_box
      height 70px
      .nav_logo
        width auto
        height 70px
        line-height 70px
        img
          margin-top -8px
      .nav
        display block
      .nav_icon
        display none
    .mobile_navBox,.mobile_nav
      display none
  @media (min-width :950px)
    .nav_box
      .nav
        li
          a
            padding 0 40px
  @media (min-width :1140px)
    .nav_box
      width 1140px

  /* nav 结束 */